{
    "name": "ijodkor/guard",
    "description": "Role, permission and action access control",
    "type": "library",
    "keywords": [
        "php",
        "laravel",
        "role",
        "permission",
        "action",
        "level",
        "access"
    ],
    "autoload": {
        "psr-4": {
            "Ijodkor\\Guard\\": "src/",
            "Ijodkor\\Guard\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Anvar Zaripboyev",
            "email": "anvarindastries@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/contracts": "^v10.0.1|^v11.17.0|^v12.10",
        "illuminate/database": "^v9.10|^v10.10|^v11.17|^v12.10",
        "ijodkor/laravel-api-response": "^1.2|^1.3",
        "spatie/laravel-translatable": "^5.2|^6.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.2.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijodkor\\Guard\\GuardServiceProvider"
            ]
        }
    }
}
